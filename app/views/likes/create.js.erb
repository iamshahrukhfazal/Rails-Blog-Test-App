<%if(@like_class)%>
    $("#post_comment").empty().append(`<%= j render @like.likeable.post.comments.where(parent_id: nil).order(id: :desc)%>`);
<%else%>
    $("#likecount_<%=@like.likeable.id%>_<%=@like.likeable.user.id%>").empty().append(`<%= j render partial: "likes/likeBtn", locals:{likeable:@like.likeable}%>`);
<%end%>
   